<Window x:Class="BiografProjekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:biografProjekt="clr-namespace:BiografProjekt"
        mc:Ignorable="d"
        Title="BiografProjekt">
    <!-- Baggrundsfarve -->

    <Grid>
        <ListBox ItemsSource="{Binding Source={x:Static biografProjekt:DomainModel.Instance} }"></ListBox>
        <TextBox Text="{Binding Source={x:Static biografProjekt:DomainModel.Instance}, Path=Movies.GetAllMovie, Mode=OneTime}"></TextBox>
    </Grid>
</Window>